{
  "name": "kylewheeler.co.uk",
  "version": "0.0.0",
  "description": "Kyle Wheeler personal website",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack -p --config webpack.config.prod.js --profile --progress --colors",
    "dev": "webpack-dev-server --config webpack.config.js --no-info --profile --colors --host 0.0.0.0 --port 8080",
    "ship": "ship dist -to gh-pages",
    "deploy": "npm run build && npm run ship",
    "start": "npm run dev"
  },
  "author": {
    "name": "Henrique Alves",
    "email": "henriquea@gmail.com",
    "url": "http://alves.im"
  },
  "license": "ISC",
  "dependencies": {
    "react": "^0.13.3",
    "webfontloader": "^1.6.6"
  },
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.16.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-loader": "^0.3.0",
    "react-hot-loader": "^1.2.9",
    "ship": "^0.2.5",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
